#include <iostream>

using namespace std;

int main()
{
    int multi=0, n=0, x=25;
    cout<<"Calcule la tabla de multiplicar de cualquier valor: ";
    cin>>n;
    for (int i=1; i<x; i++){
        multi= n*i;
        cout<<n<<"*"<<i<<"="<<multi<<endl;
    }
    return 0;
}
