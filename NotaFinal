#include <iostream>

using namespace std;

int main()
{
    float primerparcial, segundoparcial,tercerparcial, parciales, proyecto, final, notafinal;
    
    cout<<"ingresa la nota del primer parcial: ";
    cin>> primerparcial;
    cout<<"ingresa la nota del segundo parcial: ";
    cin>> segundoparcial;
cout<<"ingresa la nota del tercer parcial: ";
    cin>> tercerparcial;
    parciales=(primerparcial+segundoparcial+tercerparcial)/3;
     cout<< "ingresar nota del proyecto:";
    cin>> proyecto;
    cout<< "ingresar nota del final:";
    cin>> final;
    notafinal=(parciales*0.4)+(proyecto*0.20)+(final*0.4);
    cout<<"la nota fial es:"<<notafinal;
    return 0;
}
